{
    "name": "heterodoks/laravel-sendy",
    "description": "Laravel package for Sendy API integration",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Heterodoks\\LaravelSendy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Heterodoks\\LaravelSendy\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/support": "^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Heterodoks\\LaravelSendy\\SendyServiceProvider"
            ],
            "aliases": {
                "Sendy": "Heterodoks\\LaravelSendy\\Facades\\Sendy"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
} 